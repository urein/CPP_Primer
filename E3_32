int main()
{
	const int sz = 10;
	vector<int> v1, v2;
	for (int i = 0; i < sz; i++)
		v1.push_back(i);

	for (int i = 0; i < sz; i++)
		v2.push_back(v1[i]);

	for (auto i : v2)
		cout << i << " ";
	cout << endl;

	return 0;
}
